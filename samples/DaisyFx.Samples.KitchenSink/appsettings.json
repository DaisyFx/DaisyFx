{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Daisy":{
    "Mode": "console"
  },
  "KitchenSink":{
    "Cron": {
      "CronExpression": "*/5 * * * * *",
      "IncludeSeconds": true
    },
    "GenerateArray":{
      "Start": 10,
      "Count": 10
    },
    "LogDateTime": {
      "DateFormat": "MM/dd/yyyy"
    }
  }
}
